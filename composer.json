{
    "name": "drnkwati/repack-validation",
    "description": "Illuminate Validation Package With Minimal Dependencies.",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Nkwati Divine",
            "email": "drnkwati@gmail.com",
            "homepage": "http://dnkwati.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/container": "4.1.*",
        "drnkwati/repack-translation": "dev-master"
    },
    "require-dev": {
        "illuminate/database": "4.1.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\": "src/Illuminate/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "suggest": {
        "Illuminate/Support": "You must install Illuminate\\Support >=4.1. We avoid requiring in composer.json to provide developer flexibility.",
        "drnkwati/repack": "You may install drnkwati\\repack as alternative to Illuminate\\Support."
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
